{
  "name": "@big0290/sv-prj-helper-ui",
  "version": "0.1.0",
  "description": "Modern Svelte 5 UI library with glassmorphism design and full accessibility",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Big0290/sv-prj-helper.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "svelte": "./index.js",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "keywords": [
    "svelte",
    "svelte5",
    "ui-library",
    "components",
    "glassmorphism",
    "accessible",
    "a11y"
  ],
  "license": "MIT",
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.19.1",
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/commands": "^6.10.0",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/search": "^6.5.11",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.6",
    "@zxing/library": "^0.21.3",
    "jsbarcode": "^3.12.1",
    "jsqr": "^1.4.0",
    "marked": "^16.4.1",
    "prismjs": "^1.30.0",
    "qrcode": "^1.5.4"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "svelte": "./index.js",
      "import": "./index.js",
      "default": "./index.js"
    },
    "./package.json": "./package.json",
    "./styles": {
      "import": "./styles/theme.css",
      "default": "./styles/theme.css"
    },
    "./ui": {
      "types": "./ui/index.d.ts",
      "svelte": "./ui/index.js",
      "import": "./ui/index.js",
      "default": "./ui/index.js"
    },
    "./docs": {
      "types": "./docs/index.d.ts",
      "import": "./docs/index.js",
      "default": "./docs/index.js"
    },
    "./hooks": {
      "types": "./hooks/index.d.ts",
      "import": "./hooks/index.js",
      "default": "./hooks/index.js"
    }
  }
}